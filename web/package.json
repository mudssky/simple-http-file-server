{
  "name": "web",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "proxy:dev": "cross-env VITE_PROXY_TARGET='http://127.0.0.1:7888' vite --host",
    "build": "tsc && vite build",
    "lint": "eslint --cache .",
    "format": "prettier --write --cache .",
    "preview": "vite preview",
    "prepare": "cd .. && husky install web/.husky",
    "rome:check": "rome check .",
    "rome:fix": "rome check --apply .",
    "rome:fixunsafe": "rome check --apply-unsafe .",
    "commit": "git add -A && git cz",
    "push": "pnpm commit && git push",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache .",
      "rome format --write"
    ],
    "!*.{ts,tsx,js,jsx}": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./web/node_modules/cz-git"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "~5.8.5",
    "aplayer": "^1.10.1",
    "artplayer": "^5.0.9",
    "artplayer-plugin-danmuku": "^5.0.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-view": "^1.2.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.7.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "rome": "^12.1.3",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.34.3"
  },
  "packageManager": "pnpm@8.6.6"
}
