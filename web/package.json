{
  "name": "web",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "npm run dev",
    "proxy:dev": "cross-env VITE_PROXY_TARGET='http://127.0.0.1:7888' vite --host",
    "build": "tsc && vite build",
    "lint": "eslint --cache .",
    "format": "prettier --write --cache .",
    "preview": "vite preview",
    "prepare": "cd .. && husky install web/.husky",
    "biome:check": "biome check .",
    "biome:fix": "biome check --apply .",
    "biome:fixunsafe": "biome check --apply-unsafe .",
    "commit": "git add -A && git cz",
    "push": "pnpm commit && git push",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache .",
      "biome format --write"
    ],
    "!*.{ts,tsx,js,jsx}": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./web/node_modules/cz-git"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "~5.13.1",
    "aplayer": "^1.10.1",
    "artplayer": "^5.1.1",
    "artplayer-plugin-danmuku": "^5.0.1",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-view": "^1.2.4",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "react-use": "^17.4.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^1.2.0"
  },
  "packageManager": "pnpm@8.11.0+sha256.5858806c3b292cbec89b5533662168a957358e2bbd86431516d441dc1aface89"
}
