{
  "name": "web",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "npm run dev",
    "proxy:dev": "cross-env VITE_PROXY_TARGET='http://127.0.0.1:7888' vite --host",
    "build": "tsc && vite build",
    "lint": "eslint --cache .",
    "format": "prettier --write --cache .",
    "preview": "vite preview",
    "prepare": "cd .. && husky install web/.husky",
    "biome:check": "biome check .",
    "biome:fix": "biome check --apply .",
    "biome:fixunsafe": "biome check --apply-unsafe .",
    "commit": "git add -A && git cz",
    "push": "pnpm commit && git push",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache .",
      "biome format --write"
    ],
    "!*.{ts,tsx,js,jsx}": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./web/node_modules/cz-git"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "~5.10.1",
    "aplayer": "^1.10.1",
    "artplayer": "^5.0.9",
    "artplayer-plugin-danmuku": "^5.0.1",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-view": "^1.2.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.2.2",
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.7.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.6.6"
}
